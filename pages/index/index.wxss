.container{
  
  margin: 20rpx;
}

.banner{
  width: 750rpx;
}

.title{
  padding: 6px 10px;
  border: 1px solid #e64340;
  border-radius: 5px;
  color: #e64340;
  margin: 1.5em 0 1em 0;
}

.footer{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  color: #999;
  font-size: 12px;
  margin: 1em;
}


.conut{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-top: 2em;
}

.conut_in{
  width:235rpx;
  text-align: center;
  color: #999;
  line-height: 2em;
  font-size:14px;
}

.right_line{
  border-right: 1px solid #ccc;
}

.center{
  display: flex;
  justify-content: center;
  text-align: center;
}

.rule{
  font-size: 15px;
  line-height: 1.5em;
  margin-bottom: 0.5em;
  color: #666;
}

.logo, .man{
  display: flex;
  flex-direction: column;
}

.logo_row, .man_row{
  display: flex;
  flex-direction: row;
}

.logo_container,.man_container{
  width: 335rpx;
  margin: 0 10rpx 1em 10rpx;
  color: #333;
  font-size: 14px;
  line-height: 1.5em;
}

.logo_container image{
  width: 345rpx;
  height: 210rpx;
}

.man_container image{
  width:230rpx;
  height: 230rpx;
  margin-bottom: 10rpx;
}

.name{
  font-weight: bold;
}

.submit{
  margin:2em 0;
}

